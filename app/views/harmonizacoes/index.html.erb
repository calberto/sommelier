<p id="notice"><%= notice %></p>

<h1>Listando Harmonizações</h1>

<table>
  <thead>
    <tr>
      <th>Vinho</th>
      <th>Prato</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @harmonizacoes.each do |harmonizaco| %>
      <tr>
        <td><%= harmonizaco.vinho_id %></td>
        <td><%= harmonizaco.prato_id %></td>
        <td><%= link_to 'Show', harmonizaco %></td>
        <td><%= link_to 'Edit', edit_harmonizaco_path(harmonizaco) %></td>
        <td><%= link_to 'Destroy', harmonizaco, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @harmonizacoes %>

<%= link_to 'Novo Usuário', new_usuario_path %>
